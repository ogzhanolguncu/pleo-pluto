{
  "name": "pleo-space-rockets-app",
  "version": "1.0.0",
  "contributors": [
    "Maciek Pekala <maciek@pleo.io>",
    "Peter Scott <peter@pleo.io>"
  ],
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@chakra-ui/react": "^1.0.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "framer-motion": "^4.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.8",
    "react-router": "^6.0.0-alpha.3",
    "react-router-dom": "^6.0.0-alpha.3",
    "react-scripts": "3.4.3",
    "react-use": "^17.3.2",
    "swr": "^0.3.0",
    "timeago.js": "^4.0.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "prepare": "husky install",
    "e2e-test": "cypress open"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-console": "warn",
      "no-unneeded-ternary": "error",
      "@typescript-eslint/no-unused-vars": "error",
      "eqeqeq": "error"
    },
    "ignorePatterns": [
      "src/**/*.d.ts",
      "node_modules/"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx},!src/**/*.d.ts": [
      "prettier --write",
      "eslint --max-warnings=0"
    ],
    "*.{tsx,jsx,ts,js}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.44",
    "@types/react-dom": "^18.0.0",
    "cypress": "^9.5.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  }
}
